  <!-- IMU -->
  <joint name="imu_joint" type="fixed">
    <origin xyz="-0.0 0 -0.02"/>
    <parent link="Body"/>
    <child link="imu_link"/>
  </joint>

  <link name="imu_link">
    <visual>
      <origin xyz="0.0 0 0.0" rpy="0 0 0"/>
      <geometry>
        <box size="0.01 0.01 0.01"/>
      </geometry>
    </visual>
  </link>

   <!-- Driving Parts -->

  <joint name="LM" type="fixed">
    <origin xyz="-0.0 0 -0.2135" rpy="0 0 0"/>
    <!-- <origin xyz="-0.1195 0 -0.2135" rpy="0 0 0"/> -->
    <parent link="Body"/>
    <child link="DrivingPart"/>
    <axis xyz="1 0 0"/>
    <limit effort="5000" lower="-0.3" upper="0.3" velocity="20"/>
  </joint>

  <link name="DrivingPart">
    <inertial>
      <!-- <origin xyz="-0.049 0 -0.10847" rpy="0 0 0"/> -->
      <origin xyz="-0.0 0 -0.10847" rpy="0 0 0"/>
      <mass value="38.72018"/>
      <inertia ixx="1.30793521" ixy="-0.004952681" ixz="0.198291931" 
               iyy="1.154265591" iyz="-0.005973557" izz="1.323768099"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://quadruped_test_description/mesh/V2/DrivingPart.stl" scale="1 1 1"/>
      </geometry>
    </visual>
  </link>

  <joint name="LW" type="continuous">
    <origin xyz="0 0.2693 -0.1105" rpy="0 0 0"/>
    <parent link="DrivingPart"/>
    <child link="LeftWheel"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="LeftWheel">
    <inertial>
      <origin xyz="0 0.02981 0" rpy="0 0 0"/>
      <mass value="12.05514"/>
      <inertia ixx="0.175462932" ixy="3.93246E-06" ixz="-8.1921E-07" 
               iyy="0.189170789" iyz="6.85486E-06" izz="0.175465175"/>
    </inertial>
    <visual>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <geometry>
        <mesh filename="package://quadruped_test_description/mesh/V2/LeftWheel.stl" scale="1 1 1"/>
      </geometry>
    </visual>
