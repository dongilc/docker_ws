  </link>

  <joint name="RW" type="continuous">
    <origin xyz="0 -0.2693 -0.1105" rpy="0 0 0"/>
    <parent link="DrivingPart"/>
    <child link="RightWheel"/>
    <axis xyz="0 1 0"/>
  </joint>

  <link name="RightWheel">
    <inertial>
      <origin xyz="0 -0.02981 0" rpy="0 0 0"/>
