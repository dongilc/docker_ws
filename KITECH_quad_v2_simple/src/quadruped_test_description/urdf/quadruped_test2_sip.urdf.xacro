      <collision>
        <origin xyz="-0.0 0 -0.02" rpy="0 0 0"/>
        <geometry>
          <box size="2.4 0.4 0.28"/>
        </geometry>
      </collision>

      <collision>
        <origin xyz="-0.0 0 0.1" rpy="0 0 0"/>
        <geometry>
          <box size="2.4 0.8 0.035"/>
        </geometry>
      </collision>
  </link>

