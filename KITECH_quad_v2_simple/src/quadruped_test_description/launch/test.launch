<launch>
    <arg name="model"/>
    <param name="robot_description" textfile="$(find quadruped_test_description)/urdf/quadruped_test2.urdf"/>
    <param name="use_gui" value="ture"/>
    <node name="joint_state_publisher" pkg="joint_state_publisher" type="joint_state_publisher"/>
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher"/>
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find quadruped_test_description)/urdf.rviz" required="True"/>
</launch>
